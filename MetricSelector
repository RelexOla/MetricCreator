MetricRenderer.all_fields.select{ |f|
    f.name == :customer_test_metric_ola
}.first.options[:tql] = "sum(daily_values.open) && level[user,products](sum(locations.code == '9000' && (daily_values.balance > 0) || 0) / sum(locations.code == '9000' && (daily_values.date >= product_locations#getFirstBalanceOrDeliveryDate)))"

