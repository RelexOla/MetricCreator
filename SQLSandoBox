SELECT mi.TUOTEAVAIN as product, org.MORMAID as location, SUBSTR(mi.KIRJAUSAIKA,1,10) as date, SUBSTR(mi.KIRJAUSAIKA,12,14) as impulse_time, mi.TYYPPITUNNUS as impulse_type, mi.IMPULSSIID as "code:additional_key", mi.LISATIEDONARVO as impulse FROM RELEX.MARTTIMYYMALAIMPULSSI as mi INNER JOIN RELEX.MARTTIORGANISAATIO as org ON mi.MARTTIKYID = org.MARTTIID WHERE IMPULSSINAIKA > current date - 7 days AND mi.TYYPPITUNNUS in ('EP1', 'EP2', 'EP3', 'TP1', 'TP2', 'TP3', 'TP4');