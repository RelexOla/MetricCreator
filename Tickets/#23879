$ui_config.custom_extend(MetricRenderer) do

MetricRenderer.all_fields.each{ |f|

f.label.gsub! 'Product', 'SKU';
f.label.gsub! 'product', 'SKU';
f.label.gsub! 'Products', 'SKUs';
f.label.gsub! 'products', 'SKUs';

}
end

$ui_config.Renderers

$ui_config.custom_extend(Dimension) do
    Dimension.all.each do |d|;
    d[1].label.gsub! 'Product', 'SKU';
    d[1].label.gsub! 'Product', 'SKU';
    d[1].label.gsub! 'Product', 'SKU';
    d[1].label.gsub! 'Product', 'SKU';
    end
end;
;

$ui_config.custom_extend(ProductRenderer) do
    ProductRenderer.all_fields.each{ |f|
        f.label.gsub! 'Product', 'SKU';
        f.label.gsub! 'product', 'SKU';
        f.label.gsub! 'Products', 'SKUs';
        f.label.gsub! 'products', 'SKUs';
    }
end;

$ui_config.custom_extend(Renderer) do
    self.all.each do |renderer|
        renderer.all_fields.each{ |f|
            f.label.gsub! 'Product', 'SKU';
            f.label.gsub! 'product', 'SKU';
            f.label.gsub! 'Products', 'SKUs';
            f.label.gsub! 'products', 'SKUs';
        }
    end
    self.all.each do |f|
        f.label.gsub! 'Product', 'SKU';
        f.label.gsub! 'product', 'SKU';
        f.label.gsub! 'Products', 'SKUs';
        f.label.gsub! 'products', 'SKUs';
    end
end

$ui_config.custom_extend(Renderer) do
    self.all.each do |f|
        f.label.gsub! 'Product', 'SKU';
        f.label.gsub! 'product', 'SKU';
        f.label.gsub! 'Products', 'SKUs';
        f.label.gsub! 'products', 'SKUs';
    end
end



    all_renderers =
        Renderer.all_parameters + Renderer.custom_renderers +
            Renderer.all_optimizations + Renderer.custom_optimization_actions +
            Renderer.all_batch_runs +
            Renderer.all_jobs

    all_renderers.each do |renderer|
      renderer.all_fields.each{ |f|
        f.label.gsub! 'Product', 'SKU';
        f.label.gsub! 'product', 'SKU';
        f.label.gsub! 'Products', 'SKUs';
        f.label.gsub! 'products', 'SKUs';
      }
      renderer.label.gsub! 'Product', 'SKU';
      renderer.label.gsub! 'product', 'SKU';
      renderer.label.gsub! 'Products', 'SKUs';
      renderer.label.gsub! 'products', 'SKUs';
    end

        custom_extend(Renderer) do
          self.all.each do |renderer|
            renderer.all_fields.each{ |f|
              f.label.gsub! 'Product', 'SKU';
              f.label.gsub! 'product', 'SKU';
              f.label.gsub! 'Products', 'SKUs';
              f.label.gsub! 'products', 'SKUs';
            }
          end
          self.all.each do |f|
            f.label.gsub! 'Product', 'SKU';
            f.label.gsub! 'product', 'SKU';
            f.label.gsub! 'Products', 'SKUs';
            f.label.gsub! 'products', 'SKUs';
          end
        end